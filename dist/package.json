{
    "name": "react-npm-minimal-package",
    "version": "1.1.1",
    "description": "Minimal project skeleton designed for easy development of React components to be later published on NPM",
    "main": "index.js",
    "scripts": {
	"test": "ava",
	"prepublish": "webpack -p --progress --colors --config webpack.config.publish.js",
	"demo": "webpack -p --progress --colors --env=prod",
	"dev": "webpack-dev-server --inline --env=dev"
    },
    "repository": {
	"type": "git",
	"url": "git+https://github.com/nukeop/react-npm-minimal-package.git"
    },
    "keywords": [
	"boilerplate",
	"npm",
	"react"
    ],
    "author": "nukeop",
    "license": "MIT",
    "devDependencies": {
	"ava": "^0.25.0",
	"babel-core": "^6.26.0",
	"babel-loader": "^7.1.2",
	"babel-preset-env": "^1.6.1",
	"babel-preset-react": "^6.24.1",
	"css-loader": "^0.28.8",
	"node-sass": "^4.7.2",
	"react-hot-loader": "^4.2.0",
	"sass-loader": "^7.0.2",
	"style-loader": "^0.21.0",
	"webpack": "^4.10.2",
	"webpack-dev-server": "^3.1.4"
    },
    "dependencies": {
	"react": "^16.2.0",
	"react-dom": "^16.2.0"
    }
}
